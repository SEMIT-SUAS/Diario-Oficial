{
  "name": "dom",
  "version": "1.0.0",
  "description": "DOM - DiÃ¡rio Oficial Municipal - Sistema de gestÃ£o de publicaÃ§Ãµes oficiais",
  "main": "backend.ts",
  "scripts": {
    "dev": "vite",
    "server": "tsx src/server-pg.ts",
    "api": "tsx backend.ts",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "start:api": "npm run server",
    "start:front": "npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:init": "node scripts/init-db.js",
    "db:reset": "npm run db:init",
    "db:check": "tsx -e \"import { testConnection } from './src/config/database'; testConnection().then(console.log).catch(console.error)\"",
    "db:console": "psql -U postgres -d dom_database",
    "clean": "rm -rf dist node_modules/.cache",
    "clean:all": "rm -rf dist node_modules package-lock.json yarn.lock",
    "install:clean": "npm run clean:all && npm install",
    "test:api": "curl http://localhost:3001/api/health",
    "test:front": "curl http://localhost:3000",
    "test:login": "curl -X POST http://localhost:3001/api/auth/login -H \"Content-Type: application/json\" -d '{\"email\":\"admin@municipio.gov.br\",\"password\":\"admin123\"}'",
    "dev:pg": "echo \"Use: npm run server (nÃ£o inicializa banco) ou npm run db:init (inicializa banco)\"",
    "dev:sandbox": "wrangler pages dev dist --d1=dom-production --local --ip 0.0.0.0 --port 3000",
    "deploy": "npm run build && wrangler pages deploy dist",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name dom",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:migrate:local": "wrangler d1 migrations apply dom-production --local",
    "db:migrate:prod": "wrangler d1 migrations apply dom-production",
    "db:seed": "wrangler d1 execute dom-production --local --file=./seed.sql",
    "db:reset:old": "rm -rf .wrangler/state/v3/d1 && npm run db:migrate:local && npm run db:seed",
    "db:console:local": "wrangler d1 execute dom-production --local",
    "db:console:prod": "wrangler d1 execute dom-production",
    "clean-port": "fuser -k 3000/tcp 2>/dev/null || true",
    "test": "curl http://localhost:3000",
    "git:commit": "git add . && git commit -m",
    "help": "echo '\\nðŸ“‹ COMANDOS DISPONÃVEIS:\\n  npm run server        - Inicia backend API (porta 3001)\\n  npm run dev           - Inicia frontend (porta 3000)\\n  npm start             - Inicia ambos (frontend + backend)\\n  npm run db:init       - Inicializa/recria banco de dados\\n  npm run db:reset      - Alias para db:init\\n  npm run test:api      - Testa API\\n  npm run test:login    - Testa login\\n'"
  },
